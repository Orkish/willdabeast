<ul class="large-block-grid-1" id="img-model">
  <% @modelings.each do |modeling| %>
    <% if current_admin %>
      <%= button_to "delete", modeling, :method => :delete %>
    <% end %>
  <li>
    <%= image_tag modeling.image_url %>
    <h4 class="desc"><%= modeling.description %></h4>
    <% end %>
  </li>
</ul>